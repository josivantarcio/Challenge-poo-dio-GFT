package me.dio.challengeBootcamp.entities;

import java.time.LocalDate;

public class Test {
	public static void main(String[] args) {
		Course course1 = new Course();
		course1.setTitle("Curso Java");
		course1.setDescription("Programação OOP Java");
		course1.setWorkload(8);
		
		Course course2 = new Course();
		course2.setTitle("Curso Python");
		course2.setDescription("Programação OOP Python");
		course2.setWorkload(6);
		
		Mentorships mentorships1 = new Mentorships();
		mentorships1.setTitle("Mentoria Java");
		mentorships1.setDescription("Mentoria de Java para QA");
		mentorships1.setMentoringDate(LocalDate.now()); //the opening of the mentorship will be generated in the instance of the object
		
		System.out.println(course1);
		System.out.println(course2);
		System.out.println(mentorships1);
	}
}
